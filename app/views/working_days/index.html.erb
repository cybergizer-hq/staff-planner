<p id="notice"><%= notice %></p>

<h1>Working Days</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Day of week</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @working_days.each do |working_day| %>
      <tr>
        <td><%= working_day.company_id %></td>
        <td><%= working_day.day_of_week %></td>
        <td><%= link_to 'Show', working_day %></td>
        <td><%= link_to 'Edit', edit_working_day_path(working_day) %></td>
        <td><%= button_to 'Destroy', working_day, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Working Day', new_working_day_path %>
